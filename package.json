{
  "name": "lintlens",
  "displayName": "LintLens — ESLint rules made easier",
  "description": "Augment your ESLint rules in Visual Studio Code — Adds descriptions above each ESLint rule via code lenses.",
  "version": "1.0.2",
  "publisher": "ghmcadams",
  "author": {
    "name": "Gabriel McAdams",
    "email": "ghmcadams@yahoo.com"
  },
  "homepage": "https://github.com/ghmcadams/vscode-lintlens/blob/master/README.md",
  "bugs": {
    "url": "https://github.com/ghmcadams/vscode-lintlens/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ghmcadams/vscode-lintlens.git"
  },
  "license": "MIT",
  "engines": {
    "vscode": "^1.25.0"
  },
  "categories": [
    "Other"
  ],
  "galleryBanner": {
    "color": "#0F1382",
    "theme": "dark"
  },
  "keywords": [
    "lintlens",
    "eslint",
    "rules",
    "metadata",
    "annotation",
    "multi-root ready"
  ],
  "activationEvents": [
    "workspaceContains:**/.eslintrc*",
    "workspaceContains:**/package.json"
  ],
  "main": "./extension",
  "scripts": {
    "postinstall": "node ./node_modules/vscode/bin/install"
  },
  "devDependencies": {
    "vscode": "^1.1.6"
  },
  "dependencies": {
    "acorn": "^5.7.1",
    "eslint": "^5.1.0",
    "open": "0.0.5",
    "yaml-ast-parser": "0.0.40"
  }
}
